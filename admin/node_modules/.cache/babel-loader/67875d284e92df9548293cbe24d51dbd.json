;AAAA;AAAA;AAAA;AAGA,KAnBA;AAoBA,IAAA,YApBA,0BAoBA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,MAAA,CAAA,KAAA,CAAA,GAAA,mBADA;;AAAA;AACA,gBAAA,GADA;AAEA,gBAAA,MAAA,CAAA,OAAA,GAAA,GAAA,CAAA,IAAA;;AAFA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAGA;AAvBA,GAVA;AAmCA,EAAA,OAnCA,qBAmCA;AACA,SAAA,YAAA;AACA,SAAA,EAAA,IAAA,KAAA,KAAA,EAAA;AACA;AAtCA,CAAA","sourcesContent":["<template>\n  <div id=\"categoryedit\">\n    <h1>{{id ? '编辑':'新建'}}分类</h1>\n    <el-form label-width=\"100px\" @submit.native.prevent=\"save\">\n      <el-form-item label=\"上级分类\">\n        <el-select v-model=\"model.parent\">\n          <el-option v-for=\"item in parents\" :key=\"item._id\" :label=\"item.name\" :value=\"item._id\"></el-option>\n        </el-select>\n      </el-form-item>\n      <el-form-item label=\"名称\">\n        <el-input v-model=\"model.name\"></el-input>\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" native-type=\"submit\">保存</el-button>\n      </el-form-item>\n    </el-form>\n  </div>\n</template>\n\n<script>\nexport default {\n  props: {\n    id: {}\n  },\n  data() {\n    return {\n      model: {},\n      parents: []\n    };\n  },\n  methods: {\n    async save() {\n      let res;\n      if (this.id) {\n        const res = await this.$http.put(`rest/categories/${this.id}`, this.model);\n      } else {\n        const res = await this.$http.post(\"rest/categories\", this.model);\n      }\n\n      this.$router.push(\"/categories/list\");\n\n      this.$message({\n        type: \"success\",\n        message: \"保存成功\"\n      });\n    },\n    async fetch() {\n      const res = await this.$http.get(`rest/categories/${this.id}`);\n      this.model = res.data;\n    },\n    async fetchParents() {\n      const res = await this.$http.get(`rest/categories`);\n      this.parents = res.data;\n    }\n  },\n  created() {\n    this.fetchParents();\n    this.id && this.fetch();\n  }\n};\n</script>\n\n<style>\n#categoryedit {\n  width: 100%;\n}\n</style>"],"sour